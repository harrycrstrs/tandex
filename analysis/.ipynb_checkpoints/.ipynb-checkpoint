    "# Effect of pass direction on local incidence angle\n",
    "## Analysis for TDX parameters"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "from matplotlib import pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 48,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Azimuth angles for ascending and descending passes\n",
    "az_A =349.4 + 90 - 360 \n",
    "az_D = 192.0 + 90\n",
    "\n",
    "x = np.linspace(0,360,360) # axis for slope aspect in degrees"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def incidence_angle(slope,azimuth,incidence_0): # gets local incidence angle \n",
    "    slope = np.deg2rad(slope)\n",
    "    azimuth = np.deg2rad(azimuth)\n",
    "    i = np.deg2rad(incidence_0)\n",
    "    return np.rad2deg( i - slope*np.cos(np.linspace(0,2*np.pi,360) - azimuth) )"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 122,
   "metadata": {},
   "outputs": [],
   "source": [
    "def max_angle(slope):   \n",
    "# calculates largest possible local incidence angle for either ascending or descending\n",
    "    ascending = incidence_angle(slope,az_A,33)\n",
    "    descending = incidence_angle(slope,az_D,41)\n",
    "    minimum = np.minimum(ascending,descending)\n",
    "    return minimum.max()\n",
    "\n",
    "def max_aspect(slope):\n",
    "# finds the aspect angle for which the maximum local incidence angle occurs\n",
    "    ascending = incidence_angle(slope,az_A,33)\n",
    "    descending = incidence_angle(slope,az_D,41)\n",
    "    minimum = np.minimum(ascending,descending)\n",
    "    position = minimum.argmax()\n",
    "    return np.linspace(0,360,360)[position]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 78,
